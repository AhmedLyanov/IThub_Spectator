{
  "name": "ithub-spectator",
  "productName": "IThub_Spectator",
  "version": "1.0.0",
  "description": "Application for monitoring students behaviour at the PC",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "dist": "electron-builder",
    "lint": "echo \"No linting configured\""
  },
  "repository": "https://github.com/AhmedLyanov/IThub_Spectator",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "build": {
    "appId": "com.ahmedlyanov.ithubspectator",
    "productName": "IThub Spectator",
    "win": {
      "icon": "./src/assets/logo/logo.png",
      "target": "nsis"
    },
    "directories": {
      "output": "dist"
    }
  },
  "keywords": [
    "demonstration, client, Spectator"
  ],
  "author": {
    "name": "Ahmed",
    "login": "DevAhmed",
    "email": "amoshal1997@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "electron-log": "^5.4.3",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.6.2",
    "express": "^5.1.0",
    "simple-peer": "^9.11.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "electron": "36.4.0",
    "electron-builder": "^26.0.12"
  }
}
